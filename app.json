{
  "expo": {
    "name": "Zona Sul Esportes",
    "slug": "Zsul",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/icon.png",
    "scheme": "ZSul",
    "userInterfaceStyle": "automatic",
    "splash": {
      "image": "./assets/images/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/aicon.png",
        "backgroundColor": "#ffffff"
      },
      "package": "com.milbratheduardo.escola_sp"
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/images/favicon.png"
    },
    "plugins": [
      "expo-router"
    ],
    "experiments": {
      "typedRoutes": true
    },
    "owner": "milbrathdudu",
    "extra": {
      "eas": {
        "projectId": "4da4b964-df0b-48cc-b16f-71e5e35f4ec7"
      }
    }
  }
}
